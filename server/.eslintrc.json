{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended"],
    "rules": {
        "no-undef": 2,
        "no-var": 2,
        "no-const-assign": 2,
        "no-unused-vars": 2,
        "prefer-const": 2
    }
}
